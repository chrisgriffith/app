{
  "name": "phonegap-app-showcase",
  "version": "0.0.1",
  "description": "The app showcase found at http://phonegap.github.io/app",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack --progress --colors",
    "dev": "webpack-dev-server --progress --colors",
    "build": "node build_data.js",
    "build:images": "imagemin ./img ./_site/img",
    "build:production": "npm run webpack && npm run build:images",
    "start": "node build_data.js && npm run dev",
    "deploy": "npm run build:production && git subtree push --prefix _site origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phonegap/app"
  },
  "keywords": [
    "phonegap"
  ],
  "author": "Garth Braithwaite <garthdb@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/phonegap/app/issues"
  },
  "homepage": "http://phonegap.github.io/app/",
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "circular-json": "^0.3.0",
    "css-loader": "^0.23.0",
    "escape-html": "^1.0.3",
    "history": "1.17.0",
    "imagemin-cli": "^2.0.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.1",
    "scroll-behavior": "^0.3.0",
    "static-site-generator-webpack-plugin": "^1.3.0",
    "stylus-loader": "^1.4.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "yamljs": "^0.2.4"
  }
}
